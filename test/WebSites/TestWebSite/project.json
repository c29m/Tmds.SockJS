{
    "webroot": "wwwroot",
    "version": "1.0.0-*",

    "dependencies": {
        "Kestrel": "1.0.0-beta6",
        "Microsoft.AspNet.Server.IIS": "1.0.0-beta6",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-beta6",
        "Tmds.SockJS": "0.3.0-*",
        "Microsoft.AspNet.WebSockets.Server": "1.0.0-beta6",
        "Tmds.WebSockets.Sources": "1.0.0-*"
    },

    "commands": {
        "web": "Microsoft.AspNet.Hosting --config hosting.ini",
        "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:8081"
    },

    "frameworks": {
        "dnx451": { },
        "dnxcore50": { }
    },

    "publishExclude": [
        "node_modules",
        "bower_components",
        "**.xproj",
        "**.user",
        "**.vspscc"
    ],
    "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components"
    ]
}
